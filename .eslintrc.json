{
  "env": {
    "node": true,
    "es2020": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "valid-jsdoc": [
      "error",
      {
        "requireReturnDescription": false,
        "requireParamDescription": false
      }
    ]
  }
}
